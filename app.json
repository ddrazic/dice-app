{
  "expo": {
    "name": "dice",
    "slug": "dice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ddrazic.diceapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ddrazic.diceapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "a07dfb82-14cb-4fa5-bd77-b8f21179d725"
      }
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Dopusti aplikaciji ${PRODUCT_NAME} da koristi Face ID za autentifikaciju."
        }
      ]
    ]
  }
}